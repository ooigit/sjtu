<header class="last">
  <nav>
    <ul>
	  <li><h6><%= link_to "Home", root_path %></h6></li>
	  <% if signed_in? %>
	    <li><h6><%= link_to "Courses", courses_path %></h6></li>
	    <li><h6><%= link_to "Profile", current_user %></h6></li>
	    <li><h6><%= link_to "Settings", edit_user_path(current_user) %></h6></li>
	    <li><h6><%= link_to "Sign out", signout_path %></h6></li>
	  <% else %>
	    <li><h6><%= link_to "Sign in", signin_path %></h6></li>
	    <li><h6><%= link_to "Sign up", signup_path %></h6></li>
	  <% end %>
	  <li><h6><%= link_to "Help", help_path %></h6></li>
	  <li><h6><%= link_to "About", about_path %></h6></li>
	</ul>
  </nav>
</header>
